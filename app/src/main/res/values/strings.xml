    <string name="first_aid_guide04_description02">Indblæsning = 2 forsøg</string>
    <string name="first_aid_guide04_description03">Gentag til hjælp kommer/normal vejrtrækning.</string>

    <string name="first_aid_guide05_title">Modgift Næsespray</string>
    <string name="first_aid_guide05_number">5/6</string>
    <string name="first_aid_guide05_description01">HJÆLP HINANDEN!</string>
    <string name="first_aid_guide05_description02">1 ampul i hvert næsebor - hvis det ikke forsinker hjertemassage/indblæsninger ved hjertestop.</string>

    <string name="first_aid_guide06_title">Normal Vejrtrækning</string>
    <string name="first_aid_guide06_number">6/6</string>
    <string name="first_aid_guide06_description01">Læg personen på siden.</string>
    <string name="first_aid_guide06_description02">Sikre frie luftveje (2).</string>
    <string name="first_aid_guide06_description03">Giv modgift ved tegn på overdosis.</string>
    <string name="first_aid_guide06_description04">Bliv til ambulancen kommer.</string>

