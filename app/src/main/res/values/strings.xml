    <string name="first_aid_guide03_number">3/6</string>
    <string name="first_aid_guide03_description01">ER DER NORMAL VEJRTRÆKNING?</string>
    <string name="first_aid_guide03_description02">NEJ -> HJERTESTOP! -> GÅ TIL 4 + 5.</string>
    <string name="first_aid_guide03_description03">JA -> GÅ TIL 6.</string>

    <string name="first_aid_guide04_title">Kunstigt Åndedræt + Hjertemassage</string>
    <string name="first_aid_guide04_number">4/6</string>
